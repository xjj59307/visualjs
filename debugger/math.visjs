operator: pattern {
    exec plus(op = '+', parent = node) when (self.op),
    exec minus(op = '-', parent = node) when (self.op),
    exec times(op = '*', parent = node) when (self.op),
    exec divide(op = '/', parent = node) when (self.op)
}

plus: action {
    create node=tree_node(label = op),
    create tree_edge(from = parent, to = node),
    next this.left,
    next this.right
}
